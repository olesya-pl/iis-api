{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3} {SourceContext}] [{ThreadId}] ({Scope}) {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithThreadId"
    ]
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5000"
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "db": "Server=localhost;Database=contour_dev_net;Username=postgres;Password=123",
    "db-legacy": "Server=localhost;Database=ContourLegacy;Username=postgres"
  },
  "map": {
    "layers": {
      "trackObjectPosition": "https://services7.arcgis.com/4zXTkJ76130r8v23/arcgis/rest/services/positions/FeatureServer/0"
    }
  },
  "files": {
    "storage": "Folder",
    "path": "/root/storage"
  },
  "jwt": {
    "issuer": "iis",
    "signingKey": null,
    "lifeTime": "02:00:00"
  },
  "mq": {
    "hostName": "localhost"
  },
  "materialEventPublisher": {
    "targetChannel": {
      "exchangeName": "materials"
    }
  },
  "gsmWorkerUrl": "http://ml.contour.net:8000/transcribe",
  "elasticSearch": {
    "Uri": "http://localhost:9200",
    "IndexPreffix": "ont_"
  },
  "reportsAvailable": "true",
  "maxMaterialsPerOperator": {
    "value": 10
  },
  "operatorAssigner": {
    "queueName": "assign-operator"
  },

  "salt": null,
  "defaultUserName": "olya",
  "defaultPassword": "hammer69"
}