{
    "Kestrel": {
        "Endpoints": {
            "Http": {
                "Url": "http://localhost:5223"
            }
        }
    },
    "AllowedHosts": "*",
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console",
            "Serilog.Sinks.File"
        ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3} {SourceContext}] [{CorrelationId}] {Message:lj}{NewLine}{Exception}"
                }
            },
            {
                "Name": "File",
                "Args": {
                    "path": "Logs/log.txt",
                    "fileSizeLimitBytes": 104857600,
                    "rollingInterval": "Day",
                    "rollOnFileSizeLimit": true,
                    "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3} {SourceContext}] [{CorrelationId}] {Message:lj}{NewLine}{Exception}"
                }
            }
        ],
        "Enrich": [
            "FromLogContext",
            "WithThreadId",
            "WithCorrelationId"
        ]
    },
    "elasticSearch": {
        "Uri": "http://localhost:9200",
        "IndexPreffix": "ont_",
        "totalFieldsLimit": 4096,
        "DefaultLogin": "elastic",
        "DefaultPassword": "112233",
        "ScrollDurationMinutes": 2
    },
    "ConnectionStrings": {
        "db": "Server=localhost;Database=contour_dev_net;Username=postgres;Password=123"
    },
    "distribution": {
        "refreshMaterialInterval": "02:00:00"
    },
    "materialCoefficientsConsumer": {
        "sourceChannel": {
            "exchangeName": "materials",
            "queueName": "material.coefficients",
            "prefetchCount": 5,
            "routingKeys": [
                "material.coefficients"
            ]
        }
    },
    "materialCoefficientsPublisher": {
        "sourceChannel": {
            "exchangeName": "materials",
            "queueName": "material.coefficients.evaluated",
            "prefetchCount": 5,
            "routingKeys": [
                "material.coefficients.evaluated"
            ]
        }
    },
    "materialNextAssignedConsumer": {
        "sourceChannel": {
            "exchangeName": "materials",
            "queueName": "material.nextAssigned",
            "prefetchCount": 5,
            "routingKey": "material.getNextAssigned"
        }
    },
    "mq": {
        "hostName": "localhost"
    }
}